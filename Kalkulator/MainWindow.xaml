<Window x:Class="Kalkulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kalkulator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="pierwszaliczba" HorizontalAlignment="Left" Height="23" Margin="123,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="drugaliczba" HorizontalAlignment="Left" Height="23" Margin="264,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="+&#xD;&#xA;" HorizontalAlignment="Left" VerticalAlignment="Top" Width="20" Margin="124,89,0,0" RenderTransformOrigin="-3.422,-2.644" Height="16" Click="Button_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.504"/>
                    <TranslateTransform X="0.446" Y="-0.69"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button Content="-&#xD;&#xA;" HorizontalAlignment="Left" Margin="153,88,0,0" VerticalAlignment="Top" Width="29" Click="Button_Click" Height="20"/>
        <Button Content="*&#xD;&#xA;" HorizontalAlignment="Left" Margin="201,88,0,0" VerticalAlignment="Top" Width="42" Height="20" RenderTransformOrigin="-0.857,0.4" Click="Button_Click_1"/>
        <Button Content="/&#xD;&#xA;" HorizontalAlignment="Left" Margin="264,88,0,0" VerticalAlignment="Top" Width="41" Height="20" RenderTransformOrigin="-1.366,0.5"/>
        <TextBox x:Name="result" HorizontalAlignment="Left" Height="23" Margin="422,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
